<table border="solid">
    <h2>Listado de medicos</h2>
    <thead>
        <tr>
            <th>Cedula</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Especialidad</th>
            <th>Consultorio</th>
            <th>Correo</th>
            <th>Opciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each medicos}}
        <tr>
            <td>{{cedula}}</td>
            <td>{{nombres}}</td>
            <td>{{apellidos}}</td>
            <td>{{especidalidad}}</td>
            <td>{{consultorio}}</td>
            <td>{{correo}}</td>
            <td><a class="eliminar" href="medicos/eliminar/{{cedula}}">Eliminar</a></td>
        </tr>
        {{/each}} 
    </tbody>
</table>
<a href="medicos/agregar">Agregar Medicos</a>