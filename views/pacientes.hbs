<table border="solid">
    <h2>Listado de pacientes</h2>
    <thead>
        <tr>
            <th>Cedula</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Edad</th>
            <th>Telefono</th>
            <th>Opciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each pacientes}}
        <tr>
            <td>{{cedula}}</td>
            <td>{{nombre}}</td>
            <td>{{apellido}}</td>
            <td>{{edad}}</td>
            <td>{{telefono}}</td>
            <td><a class="eliminar" href="pacientes/eliminar/{{cedula}}">Eliminar</a></td>
        </tr>
        {{/each}} 
    </tbody>
</table>
<a href="pacientes/agregar">Agregar Pacientes</a>